{
  "name": "@ahr-ahr/selftest",
  "version": "1.0.0",
  "description": "Universal self-test runner with JSON output and CLI support",
  "main": "core.cjs",
  "module": "core.mjs",
  "exports": {
    ".": {
      "require": "./core.cjs",
      "import": "./core.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "selftest": "core.cjs"
  },
  "scripts": {
    "test": "node core.cjs examples/tests.json --dev"
  },
  "keywords": [
    "test",
    "universal",
    "cli",
    "unit-test",
    "cross-language"
  ],
  "author": "Ahmad Haikal Rizal",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "minimist": "^1.2.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahr-ahr/SelfTest.git"
  },
  "bugs": {
    "url": "https://github.com/ahr-ahr/SelfTest/issues"
  },
  "homepage": "https://github.com/ahr-ahr/SelfTest#readme"
}
